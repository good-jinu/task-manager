{
	"name": "notion-task-manager",
	"version": "0.1.0",
	"license": "MIT",
	"scripts": {
		"build": "pnpm --filter @notion-task-manager/web build",
		"check": "biome check && pnpm -r check",
		"check:fix": "biome check --write",
		"check:fix:unsafe": "biome check --write --unsafe",
		"dev": "pnpm --filter @notion-task-manager/web dev",
		"dev-dev": "sst dev --stage development",
		"dev-remove": "sst remove --stage development",
		"prod-deploy": "sst deploy --stage production",
		"prod-dev": "sst dev --stage production",
		"prod-remove": "sst remove --stage production"
	},
	"devDependencies": {
		"@biomejs/biome": "^2",
		"@types/aws-lambda": "8.10.159",
		"@types/node": "^20",
		"typescript": "^5"
	},
	"dependencies": {
		"sst": "^3.17"
	}
}
